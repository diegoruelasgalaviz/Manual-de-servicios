    value: product.quantity,\n      min: \"1\",\n      max: \"10\",\n      readOnly: true,\n      onChange: function onChange() {\n        return handleAddQuantity(product.quantity);\n      }\n    }), __jsx(\"span\", {\n      className: \"plus-btn\",\n      onClick: function onClick() {\n        return handleAddQuantity(product.id);\n      }\n    }, __jsx(\"i\", {\n      className: \"bx bx-plus\"\n    })))), __jsx(\"td\", {\n      className: \"product-subtotal\"\n    }, __jsx(\"span\", {\n      className: \"subtotal-amount\"\n    }, \"$\", (product.price * product.quantity).toFixed(2)), __jsx(Link, {\n      href: \"#\"\n    }, __jsx(\"a\", {\n      className: \"remove\",\n      onClick: function onClick(e) {\n        e.preventDefault();\n        handleRemove(product.id);\n      }\n    }, __jsx(\"i\", {\n      className: \"bx bx-x\"\n    })))));\n  }) : __jsx(React.Fragment, null, __jsx(\"tr\", null, __jsx(\"td\", {\n    colSpan: \"5\"\n  }, \"Empty\"))));\n};\n\nexport default Content;","map":null,"metadata":{},"sourceType":"module"}